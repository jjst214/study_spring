<mapper namespace="org.reserve.mapper.ReserveMapper">
	<!-- 
	<resultMap type="org.reserve.domain.ReserveVO" id=""></resultMap>
	<select id="selectReserve" resultType="org.reserve.domain.ReserveVO">
		select f.fno, finfo, ftype, pprice from
		 fitness f left outer join price price on f.fno = price.fno where f.fno = 6;
